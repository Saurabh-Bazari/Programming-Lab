
sett(H,NAva) :- NAva < 20,
                H = [].

sett(H,NAva,T) :- NAva >= 20,
                starter(S1,N1),
                NowAva is NAva - N1,
                NowAva >= 0,
                T = 
                sett(H1,NowAva,T),
                write(H1),write(" "),write(S1),nl,
                append(H1,[S1],H).


